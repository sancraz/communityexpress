<div class="pree_question">
    <header>
      <div class="pree_question_user">
        <div class="pree_question_user_avatar">
          <img src="http://faralam.com/ext/img/avatar.png">
        </div>
        <div class="pree_question_user_username">username</div>
      </div>
      <div class="pree_question_title">
        <div class="pree_question_title_name">
          <%= pollType.displayText.toUpperCase() %>
        </div>
        <div class="pree_question_title_date">1 week ago</div>
      </div>
      <div class="pree_question_categories_button">
        <div class="pree_question_categories_name">
          <!-- Politics -->
          <%= categories[0].displayText %>
        </div>
        <div class="pree_question_categories_count">
          <%= categories.length %>
        </div>
      </div>
      <div class="pree_question_tags_button">
        <div class="pree_question_tags_icon"></div>
        <div class="pree_question_tags_count">
          <%= tags.length %>
        </div>
      </div>
    </header>

    <div class="pree_question_expanded_menu"></div>
