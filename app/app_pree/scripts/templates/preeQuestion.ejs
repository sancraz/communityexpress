      <div class="pree_question_tags_title">TAGS</div>
      <div class="pree_question_tags_close">X</div>
      <div class="pree_question_tags_icon_wrapper">
        <div class="pree_question_tags_icon"></div>
      </div>
      <div class="pree_question_tags_block">
        <div class="pree_question_tags_item">tag</div>
        <div class="pree_question_tags_item">tag</div>
        <div class="pree_question_tags_item">tag</div>
      </div>
    </div> -->

    <section>
      <div class="pree_question_text pree_question_text">
        <%= displayText %>
      </div>
      <div class="pree_question_answer pree_question_answer_a">
        <input type="checkbox"> Yes
      </div>
      <div class="pree_question_answer pree_question_answer_b">
        <input type="checkbox"> No
      </div>
      <div class="pree_question_answer pree_question_answer_c">
        <input type="checkbox"> Not sure
      </div>
    </section>
    <footer>
      <div class="pree_question_likes_button">
        <div class="pree_question_likes_icon"></div>
        <div class="pree_question_likes_count">
          <%= likes %>
        </div>
      </div>
      <div class="pree_question_comment_button"></div>
      <div class="pree_question_share_button"></div>
      <div class="pree_question_answers"></div>
      <div class="pree_question_more"></div>
    </footer>
  </div>